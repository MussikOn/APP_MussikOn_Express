      // Verificar cdigo
      const isCodeValid =
      /* istanbul ignore next */
      (cov_1ebl7we1os().s[235]++, yield bcrypt.compare(code, storedData.userData.verificationCode));
      /* istanbul ignore next */
      cov_1ebl7we1os().s[236]++;
      if (!isCodeValid) {
        /* istanbul ignore next */
        cov_1ebl7we1os().b[64][0]++;
        cov_1ebl7we1os().s[237]++;
