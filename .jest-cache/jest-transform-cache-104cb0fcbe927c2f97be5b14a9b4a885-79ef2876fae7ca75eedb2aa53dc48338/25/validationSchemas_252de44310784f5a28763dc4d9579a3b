',\r\n    }),\r\n  userPassword: Joi.string()\r\n    .min(8)\r\n    .max(128)\r\n    .pattern(\r\n      /^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[@$!%*?&.#])[A-Za-z\\d@$!%*?&.#]{8,}$/\r\n    )\r\n    .required()\r\n    .messages({\r\n      'string.min': 'La contrase\xF1a debe tener al menos 8 caracteres',\r\n      'string.max': 'La contrase\xF1a no puede exceder 128 caracteres',\r\n      'string.pattern.base':\r\n        'La contrase\xF1a debe contener al menos una min\xFAscula, una may\xFAscula, un n\xFAmero y un car\xE1cter especial',\r\n      'any.required': 'La contrase\xF1a es requerida',\r\n    }),\r\n  // El rol puede ser 'musico' o 'eventCreator'\r\n  roll: Joi.string().valid('musico', 'eventCreator').required().messages({\r\n    'any.only': 'El rol debe ser \"musico\" o \"eventCreator\"',\r\n    'any.required': 'El rol es requerido',\r\n  }),\r\n});\r\n\r\nexport const loginSchema = Joi.object({\r\n  userEmail: Joi.string()\r\n    .email({ tlds: { allow: false } })\r\n    .required()\r\n    .messages({\r\n      'string.email': 'El email debe tener un formato v\xE1lido',\r\n      'any.required': 'El email es requerido',\r\n    }),\r\n  userPassword: Joi.string().required().messages({\r\n    'any.required': 'La contrase\xF1a es requerida',\r\n  }),\r\n});\r\n\r\nexport const updateUserSchema = Joi.object({\r\n  name: Joi.string()\r\n    .min(2)\r\n    .max(50)\r\n    .pattern(/^[a-zA-Z\xE1\xE9\xED\xF3\xFA\xC1\xC9\xCD\xD3\xDA\xF1\xD1\\s]+$/)\r\n    .optional()\r\n    .messages({\r\n      'string.min': 'El nombre debe tener al menos 2 caracteres',\r\n      'string.max': 'El nombre no puede exceder 50 caracteres',\r\n      'string.pattern.base': 'El nombre solo puede contener letras y espacios',\r\n    }),\r\n  lastName: Joi.string()\r\n    .min(2)\r\n    .max(50)\r\n    .pattern(/^[a-zA-Z\xE1\xE9\xED\xF3\xFA\xC1\xC9\xCD\xD3\xDA\xF1\xD1\\s]+$/)\r\n    .optional()\r\n    .messages({\r\n      'string.min': 'El apellido debe tener al menos 2 caracteres',\r\n      'str