(cov_1ebl7we1os().s[271]++, new Map());
// Funcin para generar cdigo de verificacin
function generateVerificationCode() {
  /* istanbul ignore next */
  cov_1ebl7we1os().f[34]++;
  cov_1ebl7we1os().s[272]++;
  return Math.floor(100000 + Math.random() * 900000).toString();
