{
  "name": "mussikon-api",
  "version": "1.0.0",
  "description": "API completa de MussikOn para conectar m√∫sicos con organizadores de eventos",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build",
    "deploy:firebase": "firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting",
    "create:indexes": "node scripts/create-indexes.js",
    "check:indexes": "node scripts/check-indexes.js",
    "deploy:indexes": "node scripts/deploy-indexes.js",
    "monitor:indexes": "node scripts/monitor-indexes.js",
    "indexes:dry-run": "node scripts/deploy-indexes.js --dry-run",
    "analyze:queries": "node scripts/analyze-queries.js",
    "monitor:performance": "node scripts/monitor-performance.js",
    "monitor:performance:once": "node scripts/monitor-performance.js --once",
    "redis:install": "npm install -g redis-cli",
    "redis:start": "redis-server",
    "redis:test": "redis-cli ping",
    "redis:monitor": "redis-cli monitor",
    "redis:stats": "redis-cli info stats",
    "redis:flush": "redis-cli flushall",
    "production:setup": "npm run redis:install && npm run redis:start",
    "production:test": "npm run redis:test && npm run monitor:performance:once",
    "test-integration": "node scripts/test-integration.js",
    "start-integration": "powershell -ExecutionPolicy Bypass -File scripts/start-integration.ps1",
    "integration:check": "npm run test-integration && npm run start-integration",
    "idrive:setup": "node scripts/setup-idrive.js",
    "idrive:setup:create": "node scripts/setup-idrive.js --create-bucket",
    "idrive:health": "curl -X GET http://localhost:10000/api/idrive/health",
    "idrive:report": "curl -X GET http://localhost:10000/api/idrive/health/report"
  },
  "keywords": [
    "mussikon",
    "api",
    "music",
    "events",
    "musicians",
    "nodejs",
    "express",
    "typescript",
    "firebase",
    "firestore"
  ],
  "author": "Jefry Astacio <jefry.astacio@mussikon.com>",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "expo-server-sdk": "^3.15.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.6.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.1",
    "redoc-express": "^2.1.0",
    "socket.io": "^4.8.1",
    "stripe": "^18.4.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.0",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/socket.io": "^3.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "axios": "^1.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MussikOn/APP_MussikOn_Express.git"
  },
  "bugs": {
    "url": "https://github.com/MussikOn/APP_MussikOn_Express/issues"
  },
  "homepage": "https://github.com/MussikOn/APP_MussikOn_Express#readme"
}
